<h1>Sign In</h1>

<%= form_tag sessions_path do %>
  <div>
    <%= label_tag :email%>
    <%= text_field_tag :email%>
  </div>
  <div>
    <%= label_tag :password%>
    <%= password_field_tag :password%>
  </div>
  <div>
    <%= submit_tag 'Sign In' %>
  </div>
<% end %>
<br>

<%= link_to "Forgot password?", new_password_path%>
